package com.shenmintech.cbp.controller.bean.res;

import io.swagger.annotations.ApiModelProperty;
import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Builder
@Setter
@Getter
@AllArgsConstructor
@NoArgsConstructor
public class Son4GetCourseBean {

  @ApiModelProperty(value = "课程ID", required = true)
  private String courseId;

  @ApiModelProperty(value = "课程名字", required = true)
  private String courseName;

  @ApiModelProperty(value = "公司名字", required = true)
  private String companyName;

  @ApiModelProperty(value = "创建日期", required = true)
  private String createTime;

  @ApiModelProperty(value = "分组数量", required = true)
  private String groupCount;

  @ApiModelProperty(value = "课程状态：0初始状态，1已经编辑了课程模板，2已经添加了小组，并同步创建了课程表", required = true)
  private String courseStatus;

  @ApiModelProperty(value = "已入组成员总数量", required = true)
  private String studentCount;

}
