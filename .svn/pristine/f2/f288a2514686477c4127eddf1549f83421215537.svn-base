package com.shenmintech.cbp.biz.vo;

public enum CourseDoneFlag {
  DONE("已完成", "1"), UNDONE("未完成", "0");

  private String status;
  private String code;

  private CourseDoneFlag(String status, String code) {
    this.status = status;
    this.code = code;
  }

  public String getStatus() {
    return status;
  }

  public void setStatus(String status) {
    this.status = status;
  }

  public String getCode() {
    return code;
  }

  public void setCode(String code) {
    this.code = code;
  }

}
