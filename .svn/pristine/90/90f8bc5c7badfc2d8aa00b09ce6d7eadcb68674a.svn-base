package com.shenmintech.cbp.controller.bean.res;

import io.swagger.annotations.ApiModelProperty;
import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Builder
@Setter
@Getter
@AllArgsConstructor
@NoArgsConstructor
public class BasicInfo {

  @ApiModelProperty(value = "员工ID", required = true)
  String employeeId;

  @ApiModelProperty(value = "员工工号", required = true)
  String employeeNo;

  @ApiModelProperty(value = "员工真实姓名", required = true)
  String realName;

  @ApiModelProperty(value = "会员类型", required = true)
  String memberType;

  @ApiModelProperty(value = "性别", required = true)
  String sex;

  @ApiModelProperty(value = "热量单位", required = true)
  String calUnit;

  @ApiModelProperty(value = "电话", required = true)
  String mobile;

  @ApiModelProperty(value = "邮箱", required = true)
  String email;
}
